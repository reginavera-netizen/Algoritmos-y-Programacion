Algoritmo DEPRECIACION
	// Datos de entrada
	Definir costo Como Entero
	Definir valorRecu Como Entero
	Definir vidaUtil Como Entero
	// Datos de salida
	Definir D Como Entero
	// Proceso 
	costo <- 20000
	valorRecu <- 2000
	vidaUtil <- 6
	D <- (costo-valorRecu)/vidaUtil
	// Salida-Resultado
	Escribir 'Depreciacion', D
FinAlgoritmo
